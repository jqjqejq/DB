CREATE TABLE "GNBWFDAT"."M_PASS_CHK_IA"  (
		  "LC_CODE" CHAR(7) NOT NULL WITH DEFAULT '' , 
		  "SECT_CODE" CHAR(7) NOT NULL WITH DEFAULT '' , 
		  "LC_NAME" CHAR(90) NOT NULL WITH DEFAULT '' , 
		  "RANK" CHAR(2) NOT NULL WITH DEFAULT '' , 
		  "PSWORD" CHAR(50) NOT NULL WITH DEFAULT '' , 
		  "PASSDATE" DATE NOT NULL WITH DEFAULT "SYSIBM"."DATE"('1900-01-01') , 
		  "AGENT_NO" CHAR(7) NOT NULL WITH DEFAULT '' , 
		  "POST_IT" CHAR(150) NOT NULL WITH DEFAULT '' , 
		  "VIOLATIONDATE" CHAR(8) NOT NULL WITH DEFAULT '' , 
		  "VIOLATIONCNT" SMALLINT NOT NULL WITH DEFAULT 0 , 
		  "REP_CODE" CHAR(5) NOT NULL WITH DEFAULT '' , 
		  "SECTCLS_CODE" CHAR(1) NOT NULL WITH DEFAULT '' , 
		  "LC_CODE_08" CHAR(8) NOT NULL WITH DEFAULT '' , 
		  "LC_NM" CHAR(60) NOT NULL WITH DEFAULT '' , 
		  "CRT_DT" TIMESTAMP NOT NULL WITH DEFAULT CURRENT TIMESTAMP , 
		  "MOD_DT" TIMESTAMP NOT NULL WITH DEFAULT CURRENT TIMESTAMP , 
		  "CRTUSR_ID" CHAR(8) NOT NULL WITH DEFAULT '' , 
		  "MODUSR_ID" CHAR(8) NOT NULL WITH DEFAULT '' )   ;

COMMENT ON TABLE "GNBWFDAT"."M_PASS_CHK_IA" IS 'ＩＡ管理マスタ';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."AGENT_NO" IS '営業所';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."CRTUSR_ID" IS 'データ作成者';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."CRT_DT" IS 'データ作成日時';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."LC_CODE" IS 'ＬＣコード';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."LC_CODE_08" IS 'ＬＣコード（８桁）';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."LC_NAME" IS '氏名';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."LC_NM" IS 'カナ氏名';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."MODUSR_ID" IS 'データ更新者';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."MOD_DT" IS 'データ更新日時';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."PASSDATE" IS 'パスワード変更日';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."POST_IT" IS '附箋';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."PSWORD" IS 'パスワード';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."RANK" IS 'ランク';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."REP_CODE" IS 'レプリケーションコード';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."SECTCLS_CODE" IS '所属分類コード';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."SECT_CODE" IS '所属';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."VIOLATIONCNT" IS '使用不可カウント数';

COMMENT ON COLUMN "GNBWFDAT"."M_PASS_CHK_IA"."VIOLATIONDATE" IS '使用不可日';


-- 表の主キーの DDL ステートメント "GNBWFDAT"."M_PASS_CHK_IA"

